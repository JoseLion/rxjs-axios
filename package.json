{
  "name": "rxjs-axios",
  "version": "0.0.0",
  "description": "A complete Axios wrapper that uses RxJS observables",
  "repository": "git@github.com:JoseLion/rxjs-axios.git",
  "homepage": "https://joselion.github.io/rxjs-axios/",
  "bugs": "https://github.com/JoseLion/rxjs-axios/issues",
  "author": "Jose Luis Leon <joseluis5000l@gmail.com> (https://github.com/JoseLion)",
  "license": "MIT",
  "keywords": [
    "ajax",
    "axios",
    "http",
    "observable",
    "reactive",
    "request",
    "rxjs",
    "rxjs-observable"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "yarn compile && yarn lint && yarn test --forbid-only",
    "compile": "tsc",
    "lint": "eslint .",
    "release": "semantic-release",
    "test": "NODE_ENV=test mocha"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@assertive-ts/core": "^2.0.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "axios": "^1.6.2",
    "eslint": "^8.54.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "form-data": "^4.0.0",
    "mocha": "^10.2.0",
    "msw": "^2.0.9",
    "rxjs": "^7.8.1",
    "semantic-release": "^22.0.8",
    "semantic-release-yarn": "^3.0.2",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "axios": ">=1.0.0",
    "rxjs": ">=5.5.12"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": false
    },
    "rxjs": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
